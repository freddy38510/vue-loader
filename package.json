{
  "name": "@freddy38510/vue-loader",
  "version": "17.1.0",
  "license": "MIT",
  "author": "Evan You",
  "repository": "freddy38510/vue-loader",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "pretest": "tsc",
    "test": "jest --coverage",
    "test:webpack5": "WEBPACK5=true jest -c --coverage",
    "dev-example": "webpack serve --config example/webpack.config.js --hot --stats minimal",
    "build-example": "rm -rf example/dist && webpack --config example/webpack.config.js --env prod",
    "build-example-ssr": "rm -rf example/dist-ssr && webpack --config example/webpack.config.js --env prod --env ssr && node example/ssr.js",
    "lint": "prettier --write --parser typescript \"{src,test}/**/*.{j,t}s\"",
    "prepublishOnly": "tsc && commit-and-tag-version -s --no-verify"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "chalk": "4.1.2",
    "hash-sum": "2.0.0",
    "loader-utils": "2.0.4"
  },
  "peerDependencies": {
    "webpack": "5.75.0"
  },
  "peerDependenciesMeta": {
    "@vue/compiler-sfc": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@intlify/vue-i18n-loader": "3.3.0",
    "@types/estree": "1.0.0",
    "@types/hash-sum": "1.0.0",
    "@types/jest": "29.2.3",
    "@types/jsdom": "20.0.1",
    "@types/loader-utils": "2.0.3",
    "@types/mini-css-extract-plugin": "1.4.0",
    "@types/webpack": "4.41.33",
    "@types/webpack-merge": "4.1.5",
    "babel-loader": "8.3.0",
    "cache-loader": "4.1.0",
    "commit-and-tag-version": "10.1.0",
    "css-loader": "5.2.7",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.2",
    "html-webpack-plugin-v5": "npm:html-webpack-plugin@5.5.0",
    "jest": "29.3.1",
    "jsdom": "20.0.2",
    "lint-staged": "13.0.3",
    "markdown-loader": "7.0.0",
    "memfs": "3.4.11",
    "mini-css-extract-plugin": "1.6.2",
    "normalize-newline": "4.1.0",
    "null-loader": "4.0.1",
    "postcss-loader": "4.3.0",
    "prettier": "2.7.1",
    "pug": "3.0.2",
    "pug-plain-loader": "1.1.0",
    "source-map": "0.7.4",
    "style-loader": "2.0.0",
    "stylus": "0.59.0",
    "stylus-loader": "4.3.3",
    "sugarss": "4.0.1",
    "ts-jest": "29.0.3",
    "ts-loader": "8.4.0",
    "ts-loader-v9": "npm:ts-loader@9.4.1",
    "typescript": "4.9.3",
    "url-loader": "4.1.1",
    "vue": "3.2.45",
    "vue-i18n": "9.2.2",
    "webpack": "4.46.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.8.0",
    "webpack5": "npm:webpack@5.75.0",
    "yorkie": "2.0.0"
  },
  "resolutions": {
    "pretty-error": "^4.0.0",
    "watchpack": "^2.0.0"
  }
}
